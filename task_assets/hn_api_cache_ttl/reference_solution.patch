--- /dev/null
+++ b/Packages/Common/Sources/Common/Network/HNApiCache.swift
@@ -0,0 +1,55 @@
+import Foundation
+
+public protocol HNApiCache {
+  func get(_ key: String) -> Data?
+  func set(_ key: String, data: Data)
+}
+
+public protocol HNApiClock {
+  func now() -> Date
+}
+
+public struct SystemClock: HNApiClock {
+  public init() {}
+
+  public func now() -> Date {
+    Date()
+  }
+}
+
+public final class HNApiInMemoryCache: HNApiCache {
+  private struct Entry {
+    let data: Data
+    let expiresAt: Date
+  }
+
+  private let ttl: TimeInterval
+  private let clock: HNApiClock
+  private let lock = NSLock()
+  private var storage: [String: Entry] = [:]
+
+  public init(ttlSeconds: TimeInterval, clock: HNApiClock = SystemClock()) {
+    self.ttl = ttlSeconds
+    self.clock = clock
+  }
+
+  public func get(_ key: String) -> Data? {
+    lock.lock()
+    defer { lock.unlock() }
+    guard let entry = storage[key] else {
+      return nil
+    }
+    if entry.expiresAt <= clock.now() {
+      storage.removeValue(forKey: key)
+      return nil
+    }
+    return entry.data
+  }
+
+  public func set(_ key: String, data: Data) {
+    lock.lock()
+    defer { lock.unlock() }
+    let expiresAt = clock.now().addingTimeInterval(ttl)
+    storage[key] = Entry(data: data, expiresAt: expiresAt)
+  }
+}
